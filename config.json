{
  "seed": 42,
  "data": {
    "train_dir": "data/train",
    "val_dir": "data/val",
    "img_size": 224,
    "num_workers": 2
  },
  "train": {
    "batch_size": 32,
    "epochs": 5,
    "lr": 0.001,
    "weight_decay": 0.0001,
    "mixed_precision": true
  },
  "model": {
    "backbone": "resnet18",
    "pretrained": false,
    "num_classes": 2,
    "dropout": 0.2
  },
  "output": {
    "dir": "outputs",
    "save_best_by": "val_auc"
  }
}